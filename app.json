{
  "expo": {
    "name": "iskolar",
    "slug": "iskolar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "iskolar-scheme",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#7F68FD"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Space-Grotesk.ttf",
            "./assets/fonts/Satoshi.ttf",
            "./assets/fonts/Satoshi-Italic.ttf"
          ]
        }
      ],
      "expo-router"
    ]
  }
}
